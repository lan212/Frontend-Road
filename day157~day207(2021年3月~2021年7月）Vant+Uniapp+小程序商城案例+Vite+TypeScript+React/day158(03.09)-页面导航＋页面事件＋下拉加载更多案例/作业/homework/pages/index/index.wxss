/* 首页 */

/* 轮播图图carousel栏 start */
.carousel {
  height: 350rpx;
}
.carousel image {
  width: 100%;
  height: 100%;
}
/* 轮播图图carousel栏 end */

/* 九宫格菜单menu栏 start */
.menu {
  display: flex;
  flex-wrap: wrap;
  border-top: 1px solid #efefef;
}

.menu > navigator {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 33.33%;
  height: 200rpx;
  text-align: center;
  font-size: 24rpx;
  box-sizing: border-box;
}

.menu > navigator {
  border-right: 1px solid #efefef;
  border-bottom: 1px solid #efefef;
}

.menu > navigator:nth-child(3n) {
  border-right: none;
}

.menu image {
  margin: 0 auto;
  width: 60rpx;
  height: 60rpx;
}

.menu text {
  margin-top: 10rpx;
}
/* 九宫格菜单menu栏 end */

/* 更多链接links栏 start */
.links {
  display: flex;
  justify-content: space-around;
  margin: 20rpx 0;
}

.links view {
  width: 328rpx;
  height: 182rpx;
  background-repeat: no-repeat;
  background-size: cover;
}
/* 更多链接links栏 end */